print('Hello, i am the mind reading robot')

name=input('what is your name?')

age=input('how old?')
if name.lower()=='jenn':
    print('not again ew get outta here')
elif name.lower()=='les':
    print(f"little Les is literally, {age}, and TOADS stinks")
elif name.lower()=='r' :
    print(f"{age} years old, {name}, you love tacos...and your boyfriend is WILL ")
elif name.lower=='rl':
    print('pew... you need to take a bath')
elif name.lower()=='rp':
    print('that is a wierd name')
else:
    print('none detected')
print('im done')
